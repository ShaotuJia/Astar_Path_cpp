set(GTEST_SHUFFLE 1)

add_executable(
    Astar-test
    main.cpp
    test.cpp
    ../include/Path.cpp
    )

include_directories(
    ${CMAKE_SOURCE_DIR}/include
)


target_include_directories(Astar-test PUBLIC ../vendor/googletest/googletest/include)
target_link_libraries(Astar-test PUBLIC gtest)
